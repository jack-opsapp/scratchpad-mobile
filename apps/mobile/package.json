{
  "name": "@slate/mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit",
